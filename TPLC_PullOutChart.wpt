  procedures :
   -
     name : PullOutSection
     procedure_id : 1668344346368181544
     type_code : 14
     code : |1+
      // Summary: Pulls out or re-centers the section that was clicked
      // Syntax:
      //PullOutSection (<nPosX>, <nPosY>)
      //
      // Parameters:
      // 	nPosX: <specify the role of nPosX>
      // 	nPosY: <specify the role of nPosY>
      // Return value:
      // 	None
      //
      
      PROCEDURE PullOutSection( nPosX, nPosY )
      
      // Local variables
      nSectionSubscript		is int
      
      
      // Retrieves the subscript of the section that was clicked
      nSectionSubscript = grInfoXY(CHART_ChartPullOut, grCategory, nPosX , nPosY )
      
      // No action if the subscript is "off bounds"
      IF nSectionSubscript = -1 THEN RETURN
      
      // Reverses the current status of the section in the array (pulled out => centered / centered => pulled out)
      gaaPulledOutSections[nSectionSubscript] = NOT gaaPulledOutSections[nSectionSubscript]
      
      // Pulls out (or re-centers) the section of the chart
      grPiePullOut(CHART_ChartPullOut, nSectionSubscript, cstPullingOut * gaaPulledOutSections[nSectionSubscript])
      
      // Redraws the chart
      grDraw(CHART_ChartPullOut)
